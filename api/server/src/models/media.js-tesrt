module.exports = (sequelize, DataTypes) => {
  const Media = sequelize.define('Media', {
    id: {
      allowNull: false,
      autoIncrement: true,
      primaryKey: true,
      type: DataTypes.INTEGER
    },
    image: {
      allowNull: false,
      type: Sequelize.STRING
    },
    coach_id: {
      allowNull: false,
      type: Sequelize.STRING
    },
  });
  return Media;
}; 